    "jest-dom": "^4.0.0",
    "node-sass": "^5.0.0",
    "normalize.css": "^8.0.1",
    "npm": "^7.9.0",
    "react": "^17.0.2",
    "react-animated-cursor": "^2.1.8",
    "react-apollo": "^3.1.5",
    "react-bootstrap": "^1.5.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
